{
  "name": "m3u8 URL Sniffer with Title",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": ["webRequest", "storage", "downloads", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
